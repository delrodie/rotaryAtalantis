{{ form_start(form,{'attr':{'id':'besoinRotary'}}) }}
        {{ form_label(form.besoinFormationRotary) }} <br> <em>(6 choix possible)</em>
<ul class="mt-3">
    {% for choice in form.besoinFormationRotary %}
        <li>
            <div class="form-check">
                {{ form_widget(choice,{'attr':{'class':'form-check-input'}}) }}
                {{ form_label(choice) }}
            </div>

        </li>
    {% endfor %}
</ul>
    <div class="mt-3">{{ form_row(form.minuteRotarienne) }}</div>
    <div class="mt-3">{{ form_row(form.autreMinuteRotarienne) }}</div>
    <div class="mt-3">{{ form_row(form.articlesStatutsReglements) }}</div>
    <div class="mt-3">{{ form_row(form.autreStatuts) }}</div>

<div class="text-center mt-5 d-grid gap-2 col-6 mx-auto">
    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary">Suivant</button>
</div>
{{ form_end(form) }}